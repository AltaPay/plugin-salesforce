<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    
    <service-credential service-credential-id="valitor.credentials">
        <url>https://testgateway.pensio.com</url>
        <user-id></user-id>
        <password encrypted="true" encryption-type="common.export"></password>
    </service-credential>

    <service-profile service-profile-id="valitor.profile">
        <timeout-millis>30000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>10</rate-limit-calls>
        <rate-limit-millis>10000</rate-limit-millis>
        <cb-enabled>true</cb-enabled>
        <cb-calls>10</cb-calls>
        <cb-millis>5000</cb-millis>
    </service-profile>

    <service service-id="int_valitor.service">
        <service-type>HTTPForm</service-type>
        <enabled>true</enabled>
        <log-prefix/>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>valitor.profile</profile-id>
        <credential-id>valitor.credentials</credential-id>
    </service>

</services>
