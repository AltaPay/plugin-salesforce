                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>Valitor-PaymentFail</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="valitorPaymentNotificationPipeline">
                <display-name xml:lang="x-default">Valitor payment notification URL</display-name>
                <description xml:lang="x-default">Commerce Cloud notification landing page to update an open order.</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>Valitor-PaymentNotification</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="valitorPaymentOpenPipeline">
                <display-name xml:lang="x-default">Valitor payment open URL</display-name>
                <description xml:lang="x-default">Commerce Cloud success/receipt landing page if authorization is open.</description>
