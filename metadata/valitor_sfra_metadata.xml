    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="valitorBaseProductionURL">
                <display-name xml:lang="x-default">Valitor base production URL</display-name>
                <description xml:lang="x-default">Base production URL that needs to be used for requests</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>https://[YourShopName].pensio.com</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="valitorBaseTestURL">
                <display-name xml:lang="x-default">Valitor base test URL</display-name>
                <description xml:lang="x-default">Base test URL that needs to be used for requests</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>https://testgateway.pensio.com</default-value>
            </attribute-definition>
