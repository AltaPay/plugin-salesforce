                <attribute attribute-id="valitorPaymentSuccessPipeline"/>
                <attribute attribute-id="valitorPaymentFailPipeline"/>
                <attribute attribute-id="valitorPaymentOpenPipeline"/>
                <attribute attribute-id="valitorPaymentNotificationPipeline"/>
                <attribute attribute-id="valitorWhiteListedIP"/>
